<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.freedesktop.Notifications">
    <method name="Notify">
      <arg name="app_name" direction="in" type="s"/>
      <arg name="id" direction="in" type="u"/>
      <arg name="icon" direction="in" type="s"/>
      <arg name="summary" direction="in" type="s"/>
      <arg name="body" direction="in" type="s"/>
      <arg name="actions" direction="in" type="as"/>
      <arg name="hints" direction="in" type="a{sv}"/>
      <arg name="timeout" direction="in" type="i"/>
      <arg name="return_id" direction="out" type="u"/>
    </method>
    <method name="CloseNotification">
      <arg name="id" direction="in" type="u"/>
    </method>
    <method name="GetCapabilities">
      <arg name="return_caps" direction="out" type="as"/>
    </method>
    <method name="GetServerInformation">
      <arg name="return_name" direction="out" type="s"/>
      <arg name="return_vendor" direction="out" type="s"/>
      <arg name="return_version" direction="out" type="s"/>
      <arg name="return_spec_version" direction="out" type="s"/>
    </method>
  </interface>
</node>
